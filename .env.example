# =============================================================================
# Ever Club Members App - Environment Variables
# Copy this file to .env and fill in the values
# =============================================================================

# --- Core ---
NODE_ENV=development
PORT=3001
TZ=America/Los_Angeles
SESSION_SECRET=

# --- Database ---
DATABASE_URL=
DATABASE_POOLER_URL=
DB_POOL_MAX=10
ENABLE_PGBOUNCER=false

# --- Supabase ---
SUPABASE_URL=
SUPABASE_ANON_KEY=
SERVICE_ROLE_KEY=

# --- Stripe ---
# Managed by Replit Stripe integration — keys auto-injected
# STRIPE_SECRET_KEY and STRIPE_PUBLIC_KEY are set automatically

# --- HubSpot ---
# Managed by Replit HubSpot integration — token auto-injected
HUBSPOT_PORTAL_ID=
HUBSPOT_WEBHOOK_SECRET=
HUBSPOT_MEMBERSHIP_PIPELINE_ID=
HUBSPOT_FORM_CONTACT=
HUBSPOT_FORM_MEMBERSHIP=
HUBSPOT_FORM_TOUR_REQUEST=
HUBSPOT_FORM_PRIVATE_HIRE=
HUBSPOT_FORM_EVENT_INQUIRY=
HUBSPOT_FORM_GUEST_CHECKIN=

# --- Resend (Email) ---
# Managed by Replit Resend integration — key auto-injected
RESEND_WEBHOOK_SECRET=
ADMIN_ALERT_EMAIL=
ALERT_EMAIL=

# --- Google ---
GOOGLE_CLIENT_ID=
# Google Calendar/Sheets credentials managed by Replit integration

# --- Push Notifications (Web Push / VAPID) ---
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# --- Trackman ---
TRACKMAN_WEBHOOK_SECRET=

# --- Eventbrite ---
EVENTBRITE_PRIVATE_TOKEN=

# --- Object Storage ---
PRIVATE_OBJECT_DIR=
PUBLIC_OBJECT_SEARCH_PATHS=

# --- Security & CORS ---
ALLOWED_ORIGINS=

# --- Dev / Debug (optional) ---
DEV_LOGIN_ENABLED=false
ENABLE_TEST_LOGIN=false
DEV_WEBHOOK_FORWARD_URL=
